<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".GameModes.AgainstJeffGame.AgainstJeffFragment">

<TextView
    android:id="@+id/current_player_jeff"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textSize="34sp"
    app:layout_constraintBottom_toTopOf="@+id/grid_jeff"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.5"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:text="@string/current_player_label" />

<TableLayout
    android:id="@+id/grid_jeff"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/button00_jeff"
            style="@style/gridButton_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/empty_value" />

        <Button
            android:id="@+id/button01_jeff"
            style="@style/gridButton_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/empty_value" />

        <Button
            android:id="@+id/button02_jeff"
            style="@style/gridButton_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/empty_value" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/button10_jeff"
            style="@style/gridButton_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/empty_value" />

        <Button
            android:id="@+id/button11_jeff"
            style="@style/gridButton_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/empty_value" />

        <Button
            android:id="@+id/button12_jeff"
            style="@style/gridButton_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/empty_value" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/button20_jeff"
            style="@style/gridButton_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/empty_value" />

        <Button
            android:id="@+id/button21_jeff"
            style="@style/gridButton_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/empty_value" />

        <Button
            android:id="@+id/button22_jeff"
            style="@style/gridButton_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/empty_value" />
    </TableRow>

</TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>